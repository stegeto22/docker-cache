{
  "name": "docker-cache",
  "version": "0.5.0",
  "description": "Cache Docker Images Whether Built or Pulled",
  "homepage": "https://github.com/stegeto22/docker-cache#readme",
  "main": "dist/main/index.js",
  "repository": "github:stegeto22/docker-cache",
  "author": "Kurt von Laven <kurt.von.laven@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "yarn run build:main && yarn run build:post",
    "build:main": "yarn run ncc build src/main.ts --out dist/main --minify",
    "build:post": "yarn run ncc build src/post.ts --out dist/post --minify",
    "jest:esm": "yarn node --experimental-vm-modules \"$(yarn bin jest)\" --config build/jest.config.js",
    "test": "yarn run tsc && yarn run jest:esm"
  },
  "engines": {
    "node": "24.13.1",
    "yarn": "4.12.0"
  },
  "packageManager": "yarn@4.12.0",
  "dependencies": {
    "@actions/cache": "6.0.0",
    "@actions/core": "3.0.0"
  },
  "devDependencies": {
    "@cspell/dict-win32": "2.0.3",
    "@fast-check/jest": "1.8.0",
    "@jest/globals": "30.2.0",
    "@jest/types": "30.2.0",
    "@types/eslint": "9.6.1",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.13",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "@vercel/ncc": "0.38.4",
    "@yarnpkg/sdks": "3.2.3",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "fast-check": "3.16.0",
    "jest": "30.2.0",
    "jest-junit": "16.0.0",
    "jest-mock": "30.2.0",
    "prettier": "3.1.1",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3"
  }
}
